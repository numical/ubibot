{
  "name": "@numical/bankbot",
  "version": "0.0.1",
  "description": "An example domain package demonstrating a bot for servicing a personal bank account.",
  "main": "index.js",
  "scripts": {
    "start": "node ./lib/bankbot.js",
    "start:cli": "UBIBOT_CHANNEL=CLI node ./lib/bankbot.js",
    "start:rest": "UBIBOT_CHANNEL=REST node ./lib/bankbot.js",
    "test": "node ./test/bankbot.test.js | faucet",
    "test:cli": "UBIBOT_CHANNEL=CLI node ./test/bankbot.test.js | faucet",
    "test:rest": "UBIBOT_CHANNEL=REST node ./test/bankbot.test.js | faucet"
  },
  "bin": {
    "echobot": "./lib/bankbot.js"
  },
  "author": "mike.evans@numical.com",
  "license": "MIT",
  "dependencies": {
    "@numical/ubibot-cli": "0.0.1",
    "@numical/ubibot-config": "0.0.1",
    "@numical/ubibot-core": "0.0.1",
    "tap-summary": "^4.0.0"
  },
  "devDependencies": {
    "@numical/ubibot-rest": "0.0.1",
    "faucet": "0.0.1",
    "prettier": "^1.17.0",
    "tap-min": "^2.0.0",
    "tape": "^4.10.1"
  }
}
